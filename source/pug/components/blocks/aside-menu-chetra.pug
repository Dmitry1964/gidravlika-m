mixin aside-menu-chetra()
  -
    const menu = [
      {
        name: "бульдозеры",
        href: "buldozers.html"
      },
      {
        name: "трубоукладчики",
        href: "pipes.html"
      },
      {
        name: "минипогрузчики",
        href: "mini-loaders.html"
      },
      {
        name: "погрузчики",
        href: "loaders.html"
      },
      {
        name: "экскаваторы",
        href: "exkavator.html"
      },
      {
        name: "навесное оборудование",
        href: "#",
      }
    ]

  aside.aside-menu.aside-menu--chetra
    ul.aside-menu__list
      each item in menu
        li.aside-menu__item
          a(href=item.href).aside-menu__link=item.name
